{
  "title": "Auto Action Tray",
  "id": "auto-action-tray",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13.302",
    "verified": "13.000",
    "maximum": "13.999"
  },
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "compatibility": {
          "minimum": "1.0.0.0",
          "verified": "1.12.6.0"
        }
      },
      {
        "id": "socketlib",
        "type": "module",
        "compatibility": {}
      }
    ],
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "3.3.0",
          "maximum": "4.99.99"
        }
      }
    ]
  },
  "esmodules": [
    "/scripts/module.js"
  ],
  "scripts": [
    "greensock/dist/gsap.min.js"
  ],
  "styles": [
    "/styles/styles.css"
  ],
  "socket": true,
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "loop": false,
      "flags": {}
    }
  ],
  "packs": [
    {
      "name": "aat-default-actions",
      "label": "AAT - Default Actions",
      "path": "packs/aat-default-actions",
      "type": "Item",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      },
      "system": "dnd5e"
    },
    {
      "name": "aat-action-override",
      "label": "AAT - Action Override",
      "path": "packs/aat-action-override",
      "type": "Item",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      },
      "system": "dnd5e"
    }
  ]
}